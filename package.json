{
  "name": "express-server",
  "version": "1.0.0",
  "description": "Basic server build with nodejs and express",
  "main": "index.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production npm start",
    "test": "cross-env NODE_ENV=testing mocha server/test --timeout 60000",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run start-dev\"",
    "test-with-coverage": "cross-env NODE_ENV=testing nyc --reporter=text mocha server/test --timeout 60000",
    "start": "node server/app.js",
    "start-dev": "nodemon server/app.js"
  },
  "engines": {
    "node": "10.4.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
