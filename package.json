    "colors": "^1.4.0",
    "cloudinary": "^1.25.1",
    "concurrently": "^6.4.0",
    "config": "^1.20.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "country-state-city": "^2.1.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-async-errors": "^2.1.0",
    "express-validator": "^6.6.1",
    "helmet": "^4.1.1",
